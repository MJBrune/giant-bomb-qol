{
  "description": "Provides some simple quality-of-life enhancements to giantbomb.com.",
  "manifest_version": 2,
  "name": "Giant Bomb QOL",
  "version": "1.0",

  "permissions": [
    "*://*.giantbomb.com/api/*"
  ],

  "content_scripts": [
    {
      "matches": ["*://www.giantbomb.com/videos/*"],
      "css": ["giant_bomb_qol.css"],
      "js": ["third_party/moment.min.js", "third_party/jquery-3.1.1.min.js", "video_navigator.js"]
    }
  ],

  "web_accessible_resources": ["img/prev.png", "img/next.png"]
}
